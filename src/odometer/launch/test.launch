<launch>

  <!-- args -->
  <arg name="bag_path"
    default="/home/andre/Downloads/yolo_monocular/withslam.bag"/>

  <!-- clock -->
  <param name ="/use_sim_time" value="true"/>

  <!-- nodes -->
  <!-- <node name="republish" type="republish" pkg="image_transport" output="screen" -->
  <!--   args="compressed in:=/image_raw raw out:=/image_raw" /> -->
  <node pkg="tf" type="static_transform_publisher" name="det_to_map" args="0 0 0 0 0 0 1
    detection map 100" />
  <node pkg="rosbag" type="play" name="darknet_bag" args="--clock $(arg bag_path)"/>  
  <node pkg="odometer" type="odometer_node" name="odometer" output="screen">
    <param name="img_width"  value="1292"/>
    <param name="img_height" value="964"/>
    <param name="grid_resolution" value="1000"/>
    <param name="match_box" value="35"/>
  </node>

</launch>
