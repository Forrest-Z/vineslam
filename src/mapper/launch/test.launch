<launch>

  <!-- args -->
  <arg name="bag_path"
    default="/home/andre-criis/Documents/rosbags/04_cores_tarde_utad.bag"/>

  <!-- clock -->
  <param name ="/use_sim_time" value="true"/>

  <!-- nodes -->
  <node name="republish" type="republish" pkg="image_transport" output="screen"
    args="compressed in:=/output/image_raw raw out:=/camera/image" />
  <node pkg="rosbag" type="play" name="rosbag" args="--clock $(arg bag_path)"/>  
  <node pkg="mapper" type="mapper_node" name="mapper" output="screen">
    <param name="img_width"  value="1292"/>
    <param name="img_height" value="964"/>
    <param name="match_box" value="25"/>
    <param name="filter_window" value="5"/>
    <param name="mapper_inc" value="20"/>
    <param name="model_path"
      value="/home/andre-criis/Source/coral/detect/train/train_res/mv1_25-10/output_tflite_graph.tflite"/>
    <param name="labels_path"
      value="/home/andre-criis/Source/coral/detect/train/train_res/mv1_25-10/labels.txt"/>
  </node>

</launch>
