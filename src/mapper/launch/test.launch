<launch>
  <!-- clock -->
  <param name ="/use_sim_time" value="true"/>

  <!-- nodes -->
  <node name="republish" type="republish" pkg="image_transport" output="screen"
    args="compressed in:=/camera/image out:=/camera/image_raw" />
  <node pkg="mapper" type="mapper_node" name="mapper" output="screen">
    <param name="img_width"         value="1292"/>
    <param name="img_height"        value="964"/>

    <param name="match_box"         value="40"/>
    <param name="detector_th"       value="0.25"/>

    <param name="image_left" value="/zed/zed_node/left/image_rect_color"/>
    <param name="image_right" value="/zed/zed_node/right/image_rect_color"/>
    <param name="image_depth" value="/zed/zed_node/depth/depth_registered"/>

    <param name="model_path"
      value="/home/andre-criis/Source/coral-deeplearning-ros/detect/train/train_res/SSD-MNetV1-aveleda/output_tflite_graph_edgetpu.tflite"/>
    <param name="labels_path"
      value="/home/andre-criis/Source/coral-deeplearning-ros/detect/train/train_res/SSD-MNetV1-aveleda/labels.txt"/>
  </node>

</launch>
